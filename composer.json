{
    "name": "avto-dev/identity-laravel",
    "description": "IDEntity for Laravel",
    "keywords": [
        "identity",
        "laravel"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "avto-dev",
            "homepage": "https://github.com/avto-dev"
        }
    ],
    "require": {
        "php": "^5.6 || >=7.0 <7.3",
        "avto-dev/extended-laravel-validator": "^1.2",
        "avto-dev/static-references-laravel": "^2.2",
        "laravel/framework": ">=5.4.0 <5.7.0"
    },
    "require-dev": {
        "laravel/laravel": ">=5.4.0 <5.7.0",
        "phpunit/phpunit": "~5.7.10 || ^6.4 || ~7.0",
        "mockery/mockery": "0.9.* || ~1.0",
        "symfony/var-dumper": "~3.2 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "AvtoDev\\IDEntity\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AvtoDev\\IDEntity\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "@php ./vendor/bin/phpunit --no-coverage",
        "test-cover": "@php ./vendor/bin/phpunit --coverage-html='./coverage/html'"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AvtoDev\\IDEntity\\IDEntitiesServiceProvider"
            ]
        }
    },
    "suggest": {
        "avto-dev/faker-identifiers-providers": "Identifiers providers for fzaninotto/Faker package (^1.1)"
    },
    "support": {
        "issues": "https://github.com/avto-dev/identity-laravel/issues",
        "source": "https://github.com/avto-dev/identity-laravel"
    },
    "prefer-stable": true
}
